@model IEnumerable<BTH2703.Models.Student>
<p>
    <a class ="btn btn-success" asp asp-action="Create"> Create a new Student</a>
</p>
<table class="table table-all">
    <tr>
        <th> Student Id</th>
        <th>Student Name</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@Html.DisplayFor(m => item.StudentID)</td>
            <td>@Html.DisplayFor(m => item.StudentName)</td>
        </tr>
    }

</table>